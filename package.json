{
	"name": "bookstore",
	"version": "1.0.0",
	"description": "Fendend Project",
	"main": "main.ts",
	"scripts": {
		"webpack-dev-server": "webpack-dev-server",
		"dev": "webpack-dev-server --mode=development",
		"build": "webpack --mode=production",
		"test": "jest"
	},
	"keywords": [
		"Book",
		"vue",
		"frontend",
		"e-commerce"
	],
	"jest": {
		"transform": {
			".*\\.(vue)$": "@vue/vue3-jest",
			".(ts)": "ts-jest"
		},
		"testMatch": [
			"<rootDir>/src/views/*/*.spec.ts"
		],
		"moduleFileExtensions": [
			"ts",
			"js"
		],
		"testEnvironment": "jsdom"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"author": "Ramu Akarapu",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@pinia/testing": "^0.0.13",
		"@types/jest": "^27.0.1",
		"@vue/test-utils": "^2.0.2",
		"@vue/vue3-jest": "^27.0.0-alpha.1",
		"babel-loader": "^8.2.5",
		"css-loader": "^6.7.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^27.0.5",
		"sass": "^1.54.4",
		"sass-loader": "^13.0.2",
		"style-loader": "^3.3.1",
		"ts-jest": "^27.0.4",
		"typescript": "^4.7.4",
		"vue-template-compiler": "^2.7.8",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.10.0"
	},
	"dependencies": {
		"autoprefixer": "^10.4.8",
		"postcss-loader": "^7.0.1",
		"axios": "^0.27.2",
		"core-js": "^3.24.1",
		"pinia": "^2.0.18",
		"ts-loader": "^9.3.1",
		"vue": "^3.2.37",
		"vue-loader": "^17.0.0",
		"vue-router": "^4.1.3",
		"vue-style-loader": "^4.1.3"
	}
}
